<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
  
  <bean id="harish" class="SpringFramework.impl.EmployeeImpl">
    <property name="age" value="25"/>
    <property name="name" value="Harish Kumar"/>
    <property name="role" value="MTS"/>
    <property name="skill" ref="dev"/>
  </bean>

  <bean id="dev" class="SpringFramework.impl.SkillImpl">
    <constructor-arg>
        <value type="SpringFramework.interfaces.Skill.Type">DEV</value>
    </constructor-arg>
    <property name="skills">
        <list>
            <value>Java</value>
            <value>JS</value>
            <value>Python</value>
            <value>C/C++</value>
            <value>Scala</value>
        </list>
    </property>
  </bean>
  
</beans>